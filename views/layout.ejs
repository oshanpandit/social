    <%- body %>

</main>


<!-- <%-include('_footer');%> -->


<%-script%>

<script>

<% if(flash.success && flash.success.length > 0){%>

    new Noty({
    theme:'relax',
    text: '<%=flash.success%>',
    type:'success',
    layout:'topRight',
    timeout:800
}).show();

<%}%>

<% if(flash.error && flash.error.length > 0){%>

new Noty({
theme:'relax',
text: '<%=flash.error%>',
type:'error',
layout:'topRight',
timeout:800
}).show();

<%}%>

</script>
    

</body>

</html>